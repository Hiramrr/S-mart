{
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.(js|vue)$",
    "excludePattern": "(node_modules/|docs/)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "destination": "./docs",
    "recurse": true,
    "readme": "./README.md"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}