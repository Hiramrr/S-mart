{
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$|.+\\.vue$",
    "excludePattern": "(node_modules|docs)"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "verbose": true,
    "encoding": "utf8"
  },
  "plugins": [
    "node_modules/better-docs"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "better-docs": {
      "name": "Documentaci√≥n del Proyecto"
    }
  }
}
